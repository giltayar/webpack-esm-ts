{
  "name": "@roundforest/webpack-ts-storybook",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "exports": "./lib/src/webpack-ts-storybook.js",
  "main": "./lib/src/webpack-ts-storybook.js",
  "types": "./types/src/webpack-ts-storybook.d.ts",
  "engines": {
    "node": ">=15.0.0"
  },
  "files": [
    "src",
    "lib/src",
    "types/src"
  ],
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "watch": {
    "mocha:unit": {
      "patterns": [
        "test/unit"
      ],
      "extensions": "tsx"
    }
  },
  "scripts": {
    "build": "concurrently -r 'npm:build:*'",
    "build:typescript": "rm -rf types && tsc",
    "test": "FULL_TEST=1 concurrently -r 'npm:test:*'",
    "test:mocha-parallel": "npm run mocha -- --parallel",
    "test:eslint": "eslint --color --max-warnings=0 '**/*.@(?s|?sx)'",
    "test:typescript": "tsc --noEmit --pretty",
    "mocha": "mocha --loader=babel-register-esm --no-timeouts --exit --color 'test/unit/*.test.@(?s|?sx)' 'test/integ/*.test.@(?s|?sx)' 'test/e2e/*.test.@(?s|?sx)'",
    "mocha:e2e": "mocha --loader=babel-register-esm --no-timeouts --exit --color 'test/e2e/*.test.@(?s|?sx)'",
    "mocha:unit": "mocha --loader=babel-register-esm --no-timeouts --exit --color 'test/unit/*.test.@(?s|?sx)'",
    "watch:mocha:unit": "npm-watch mocha:unit",
    "play": "rollup --watch --config play/rollup.config.mjs"
  },
  "keywords": [
    "javascript",
    "nodejs"
  ],
  "author": "Gil Tayar <giltayar@roundforest.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/roundforest/mono",
    "directory": "packages/webpack-ts-storybook-component",
    "type": "git"
  },
  "peerDependencies": {
    "@emotion/css": "*",
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "@roundforest/emotion-styled": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "@babel/plugin-transform-typescript": "^7.14.6",
    "@emotion/babel-plugin": "^11.3.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@roundforest/mocha-commons": "^2.0.0",
    "@roundforest/register-jsdom": "^1.0.6",
    "@roundforest/rollup-testkit": "^1.0.35",
    "@roundforest/webdriverio-testkit": "^1.1.37",
    "@testing-library/react": "^12.0.0",
    "@types/chai": "^4.2.20",
    "@types/chai-dom": "^0.0.10",
    "@types/chai-subset": "^1.3.3",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.0.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "@web/rollup-plugin-html": "^1.8.0",
    "babel-register-esm": "^1.1.0",
    "chai": "^4.3.4",
    "chai-dom": "^1.9.0",
    "chai-subset": "^1.6.0",
    "concurrently": "^6.2.0",
    "eslint": "^7.30.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jsdom": "16.5.3",
    "mocha": "^9.0.2",
    "npm-watch": "^0.10.0",
    "prettier": "^2.3.2",
    "rollup": "^2.52.8",
    "rollup-plugin-browsersync": "^1.3.3",
    "rollup-plugin-terser": "^7.0.2",
    "testdouble": "^3.16.1",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.43.0",
    "webpack-cli": "^4.7.2"
  }
}
